  15          /******************************************************************
  16          程序功能：       通过P1口的展示，对比说明逻辑与或非和按位与或非之间的差别。
  17                                   当QX-MCS51实验板上的S2被按下时，执行的为按位与；
  18                                   当QX-MCS51实验板上的S3被按下时，执行的为逻辑与；
  19                                   当QX-MCS51实验板上的S4被按下时，执行的为按位或；
  20                                   当QX-MCS51实验板上的S5被按下时，执行的为逻辑或。
  21          ****************************************************************************/
  22          #include <reg51.h>
  23          
  24          sbit S2=P3^0;
  25          sbit S3=P3^1;
  26          sbit S4=P3^2;
  27          sbit S5=P3^3;
  28          
  29          void main()
  30          {
  31   1              char i=1,j=2;
  32   1              while(1)
  33   1              {
  34   2                      if(S2==0)
  35   2                              P1=i&j;   //按位与，程序会将1与2均转换为二进制，对于每个位进行与运算，如果结果不为0，则为True
  36   2                                        //故此时，即为0000 0001&0000 0010=0000 0000，结果为0，QX-MCS51学习板第一个led会亮     
  37   2                      else
  38   2                              P1=0xff;
  39   2      
  40   2                      if(S3==0)
  41   2                              P1=i&&j; //逻辑与，只要i和j均不为0，则结果为真（1），第一个led会灭。
  42   2                      else
  43   2                              P1=0xff;
  44   2      
  45   2                      if(S4==0)
  46   2                              P1=i|j;  //按位或其中之一为1则为1   第一个和第二个灯灭  0000 0001&0000 0010=0000 0011
  47   2                      else
  48   2                              P1=0xff;
  49   2      
  50   2                      if(S5==0)
  51   2                              P1=i||j;
  52   2                      else
  53   2                              P1=0xff;
  54   2              }
  55   1      }
