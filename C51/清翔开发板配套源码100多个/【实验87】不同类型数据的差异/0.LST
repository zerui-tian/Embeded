  15          /******************************************************************
  16          程序功能：              该程序通过两个延时函数的不同效果，给同学们关于不同函数类型
  17                                          的数据间差异提供一感性认识。
  18                                          下载程序后，会发现八位发光二极管被点亮的时间很短，而熄灭的
  19                                          时间很长。
  20          ************************************************************************/
  21          
  22          #include<reg52.h>
  23          
  24          void delaychar()
  25          {
  26   1              unsigned char m,n;         
  27   1              for(m=255;m>0;m--)         //unsigned char型的变量，最大值可以取到255
  28   1                      for(n=10;n>0;n--); //故这种变量延时不会太长
  29   1      }
  30          
  31          void delayint()
  32          {
  33   1              unsigned char n;
  34   1              unsigned int m;
  35   1              for(m=65535;m>0;m--)  //unsigned int型的变量，最大值可以取到65535
  36   1                      for(n=10;n>0;n--);//故这种变量，延时的长度会比前者大。
  37   1      }
  38          
  39          void main()
  40          {
  41   1              while(1)
  42   1              {
  43   2                      P1=0X00;         //当函数运行到此处时，八个二极管被点亮。
  44   2                      delaychar(); //这是点亮持续的时间。
  45   2                      P1=0xff;         //当函数运行到此处时，八个二极管被熄灭。
  46   2                      delayint();      //这是熄灭的持续时间。
  47   2              }
  48   1      }
  49          //编程时，关于某变量数值类型的选择，一边参照它可能取得的最大数值。
  50          //能定义为char型的变量，就不要定义成int型，因为当定义为int型时，
  51          //程序就会在单片机的存储区中腾出两个字节的位置来存储此变量，而单片机的
  52          //存储空间毕竟是比较有限的。故一般不要随便浪费空间。


